		<dc:creator>bluetileoz</dc:creator>
		<guid isPermaLink="false">http://ozzieausband.files.wordpress.com/2014/04/dsc_0680.jpeg</guid>
		<description></description>
		<content:encoded><![CDATA[]]></content:encoded>
		<excerpt:encoded><![CDATA[]]></excerpt:encoded>
